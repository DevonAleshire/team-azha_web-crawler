<div class="search-container">
    <div class="search-row-container">
        <div class="search-button">
        {{#link-to 'dashboard'}}
            <div class="return-button">
                <a class="waves-effect waves-light btn">Perform Another Search</a>
            </div>
        {{/link-to}}  
    </div>
    <div class="search-info-container-visualization">
        <div class="history-header">
               {{model.requestData.url}}
            </div>
            <div class = "history-more-info">
                <div class="history-section">
                    <div class="section-title">Method</div> 
                    <div>{{model.requestData.searchMethod}}</div>
                </div>
                <div class="history-section"> 
                    <div class="section-title">Depth</div>  
                    <div>{{model.requestData.depth}}</div>
                </div>
                {{#if model.requestData.keyword}}
                    <div class="history-section">
                        <div class="section-title">Keyword</div>  
                        <div>{{model.requestData.keyword}}</div>
                    </div>
                {{/if}}
            </div>
    </div>
    </div>
    <div class="search-visualization"> 
        {{d3-visualization model=model.data}}
    </div>
</div>